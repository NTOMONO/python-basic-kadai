class Human:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def check_adult(self):
        if self.age > 20:
            print(f"{self.name}さんは大人です。")
        else:
            print(f"{self.name}さんは大人ではありません。")
        
people = [
    Human("太郎", 26),
    Human("次郎", 35),
    Human("花子", 15)
]

for person in people:
    person.check_adult()